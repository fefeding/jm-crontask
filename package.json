    "lodash-id": "^0.14.0",
    "lowdb": "^1.0.0",
    "mockjs": "^1.0.1-beta3",
    "moment": "^2.17.1",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "shortid": "^2.2.8",
    "showdown": "^1.8.6",
    "simplemde": "^1.11.2",
    "typeorm": "^0.2.24",
    "vconsole": "^3.3.4",
    "vue": "^2.5.0",
    "vue-codemirror": "^4.0.6",
