  "version": "4.0.4",
  "scripts": {
    "build": "easy clean && easy build prod && npm run tsc",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "clean": "easy clean all && ets clean",
    "debug": "egg-bin debug -r egg-ts-helper/register",
    "deploy": "npm run clean && npm run build && tars-deploy crontask",
    "dev": "egg-bin dev --port=8082 -r egg-ts-helper/register",
    "doc": "typedoc --out docs",
    "fix": "eslint -c ./.eslintrc.js --ext .vue,.js,.ts app/ --fix",
    "ii": "npm install --registry https://registry.npm.taobao.org",
    "kill": "easy kill",
    "lint": "eslint -c ./.eslintrc.js --ext .vue,.js,.ts app/",
    "lint:checked": "node_modules/.bin/eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "start": "egg-scripts start --port=8082 --daemon --workers=1 --title=egg-server-crontask-system",
    "stop": "egg-scripts stop --title=egg-server-crontask-system",
    "tsc": "ets && tsc -p tsconfig.json",
    "zip": "easy zip"
  },
  "dependencies": {
    "@tars/config": "^2.0.5",
    "@types/egg": "^1.5.0",
